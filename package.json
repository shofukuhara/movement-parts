  "license": "UNLICENSED",
  "scripts": {
    "sass": "sass './curtain/src/scss/style.scss':'./curtain/src/css/style.css'",
    "pug": "pug './curtain/index.pug'",
    "format": "prettier --write './curtain/index.html'",
    "watch:pug": "pug --watch './curtain/index.pug'",
    "watch:sass": "sass --watch './curtain/src/scss/style.scss' './curtain/src/css/style.css'",
    "watch": "yarn watch:pug & yarn watch:sass",
    "bs": "browser-sync start --config bs-config.js",
    "build": "yarn watch & yarn bs"
  },
  "dependencies": {
    "browser-sync": "^2.27.10",
    "json-server": "^0.17.0",
    "prettier": "^2.7.1",
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.54.8",
    "watch": "^1.0.2"
  }
}
